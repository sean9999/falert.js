{
  "name": "@code_monk/falert",
  "repository": {
    "type": "git",
    "url": "git@github.com:sean9999/falert.js.git"
  },
  "version": "1.0.0",
  "description": "Funky Alerts for the browser",
  "browser": true,
  "files": [
    "src/**/*",
    "dist/**/*",
    "tsconfig.json",
    "README.md"
  ],
  "targets": {
    "html": {
      "source": "./src/index.html",
      "context": "browser",
      "isLibrary": false
    },
    "es6": {
      "source": "./src/js/falert.ts",
      "context": "browser",
      "isLibrary": true    ,
      "outputFormat": "esmodule"
    }
  },
  "module": "dist/e6s/falert.js",
  "homepage": "https://github.com/sean9999/falert.js/wiki/Falert",
  "browserslist": "> 3%, last 2 versions, not dead",
  "staticFiles": {
    "staticPath": "src/sounds",
    "staticOutPath": "sounds"
  },
  "scripts": {
    "start": "parcel src/index.html --open",
    "build": "parcel build",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "@code_monk",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.4.0",
    "parcel-reporter-static-files-copy": "^1.3.4"
  }
}
