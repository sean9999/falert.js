{
  "name": "@code_monk/falert",
  "version": "1.0.0",
  "description": "Funky Alerts for the browser",
  "browser": true,
  "files": [
    "src/**/*",
    "dist/**/*",
    "package.json",
    "tsconfig.json"
  ],
  "source": [
    "./src/index.html"
  ],
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "browserslist": "> 3%, last 2 versions, not dead",
  "staticFiles": {
    "staticPath": "src/sounds",
    "staticOutPath": "sounds"
  },
  "author": "@code_monk",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.4.0",
    "parcel": "^2.4.0",
    "parcel-reporter-static-files-copy": "^1.3.4"
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "prepare": "npm run build",
    "check": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}